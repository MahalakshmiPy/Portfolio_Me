// Function to shuffle text with special characters
function shuffleTextWithSpecialChars(text) {
  const specialChars = '$(?/&@;(/-'; // Pool of special characters
  const chars = text.split(''); // Convert text to an array of characters
  const letters = chars.filter((char) => char !== ' '); // Filter out spaces

  // Create a new array with letters and random special characters
  const randomChars = letters.map((letter) => {
    return Math.random() > 0.5
      ? letter
      : specialChars[Math.floor(Math.random() * specialChars.length)];
  });

  // Shuffle the array of random characters
  for (let i = randomChars.length - 1; i > 0; i--) {
    const j = Math.floor(Math.random() * (i + 1));
    [randomChars[i], randomChars[j]] = [randomChars[j], randomChars[i]];
  }

  // Rebuild the text with spaces in their original positions
  let shuffledText = '';
  let charIndex = 0;
  chars.forEach((char) => {
    if (char === ' ') {
      shuffledText += ' ';
    } else {
      shuffledText += randomChars[charIndex++];
    }
  });

  return shuffledText;
}

// Attach event listeners to all elements with the 'scrambledbutton' class
const buttons = document.querySelectorAll('.scrambledbutton');

buttons.forEach((button) => {
  // Store the original text for each button
  const originalText = button.textContent;

  // Handle mouseenter (hover)
  button.addEventListener('mouseenter', () => {
    // Shuffle text every 75ms
    const interval = setInterval(() => {
      button.textContent = shuffleTextWithSpecialChars(originalText);
    }, 75);

    // Stop shuffle after 0.5 seconds
    setTimeout(() => {
      clearInterval(interval);
      button.textContent = originalText;
    }, 500);
  });
});
</script>
<script>
// Set a cookie with an expiration date
function setCookie(name, value, days) {
    const date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
    document.cookie = `${name}=${value}; expires=${date.toUTCString()}; path=/`;
}

// Get a cookie by name
function getCookie(name) {
    const cookies = document.cookie.split('; ');
    for (const cookie of cookies) {
        const [key, value] = cookie.split('=');
        if (key === name) return value;
    }
    return null;
}

// Check if the "cookiesAccepted" cookie exists
if (getCookie('cookiesAccepted')) {
    document.getElementById('cookies-box').style.display = 'none';
}

// Add click event to accept cookies
document.getElementById('accept-cookies').addEventListener('click', function () {
    setCookie('cookiesAccepted', 'true', 30); // Set for 30 days
    document.getElementById('cookies-box').style.display = 'none';
});
</script></div><script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=677bb02901538049164afdfb" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script><script src="https://cdn.prod.website-files.com/677bb02901538049164afdfb/js/unreasonable.schunk.36b8fb49256177c8.js" type="text/javascript"></script><script src="https://cdn.prod.website-files.com/677bb02901538049164afdfb/js/unreasonable.schunk.44a5ded75531cfac.js" type="text/javascript"></script><script src="https://cdn.prod.website-files.com/677bb02901538049164afdfb/js/unreasonable.f4bbd8ab.f7d96a1db4baeea3.js" type="text/javascript"></script><script>
